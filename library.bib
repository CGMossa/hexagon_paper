@article{Niederwerder2019,
  title = {Infectious Dose of {{African}} Swine Fever Virus When Consumed Naturally in Liquid or Feed},
  author = {Niederwerder, Megan C. and Stoian, Ana M.M. and Rowland, Raymond R.R. and Dritz, Steve S. and Petrovan, Vlad and Constance, Laura A. and Gebhardt, Jordan T. and Olcha, Matthew and Jones, Cassandra K. and Woodworth, Jason C. and Fang, Ying and Liang, Jia and Hefley, Trevor J.},
  year = {2019},
